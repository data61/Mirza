{
  "extends": "tslint:recommended",
  "rules": {
    "max-line-length": {
      "options": [150]
    },
    "no-console": false,
    "quotemark": [false, "single"],
    "interface-name": {
      "options": ["never-prefix"]
    },
    "object-literal-sort-keys": false,
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "never",
          "typeLiterals": "ignore"
        },
        "esSpecCompliant": true
      }
    ],
    "only-arrow-functions": false,
    "no-var-requires": false,
    "max-classes-per-file": false
  }
}