{
  "extends": "tslint:recommended",
  "rules": {
    "max-line-length": {
      "options": [120]
    },
    "no-console": false,
    "quotemark": {
      "options": ["single", "avoid-escape"]
    },
    "interface-name": {
      "options": ["never-prefix"]
    },
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "never",
          "typeLiterals": "ignore"
        },
        "esSpecCompliant": true
      }
    ]
  }
}